<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.MainActivity"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ECECEC">


        <TextView
            android:elevation="10sp"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Quotes"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="30sp"
            android:layout_margin="10sp"/>

    </LinearLayout>


    <LinearLayout
        android:layout_margin="10sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/love"
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                app:cardCornerRadius="10sp"
                android:elevation="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:layout_width="100sp"
                    android:layout_height="100sp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/love"
                        android:scaleType="centerCrop"/>



                </LinearLayout>



            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Love"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="18sp"/>


        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"

            android:layout_margin="10dp"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                app:cardCornerRadius="10sp"
                android:elevation="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:layout_width="100sp"
                    android:layout_height="100sp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/friendship"
                        android:scaleType="centerCrop"/>



                </LinearLayout>



            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Friendship"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="18sp"/>


        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                app:cardCornerRadius="10sp"
                android:elevation="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:layout_width="100sp"
                    android:layout_height="100sp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/brithday"
                        android:scaleType="centerCrop"/>



                </LinearLayout>



            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Birthday"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="18sp"/>


        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_margin="10sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                app:cardCornerRadius="10sp"
                android:elevation="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:layout_width="100sp"
                    android:layout_height="100sp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/attitude"
                        android:scaleType="centerCrop"/>



                </LinearLayout>



            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Attitude"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="18sp"/>


        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"

            android:layout_margin="10dp"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                app:cardCornerRadius="10sp"
                android:elevation="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:layout_width="100sp"
                    android:layout_height="100sp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/alone"
                        android:scaleType="centerCrop"/>



                </LinearLayout>



            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Alone"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="18sp"/>


        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                app:cardCornerRadius="10sp"
                android:elevation="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:layout_width="100sp"
                    android:layout_height="100sp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/failure"
                        android:scaleType="centerCrop"/>



                </LinearLayout>



            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Failure "
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="18sp"/>


        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_margin="10sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                app:cardCornerRadius="10sp"
                android:elevation="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:layout_width="100sp"
                    android:layout_height="100sp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/good"
                        android:scaleType="centerCrop"/>



                </LinearLayout>



            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Good"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="18sp"/>


        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"

            android:layout_margin="10dp"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                app:cardCornerRadius="10sp"
                android:elevation="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:layout_width="100sp"
                    android:layout_height="100sp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/experience"
                        android:scaleType="centerCrop"/>



                </LinearLayout>



            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Experience"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="18sp"/>


        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                app:cardCornerRadius="10sp"
                android:elevation="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:layout_width="100sp"
                    android:layout_height="100sp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/leadership"
                        android:scaleType="centerCrop"/>



                </LinearLayout>



            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Leadership"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="18sp"/>


        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_margin="10sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout

            android:layout_margin="10dp"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                app:cardCornerRadius="10sp"
                android:elevation="10sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:layout_width="100sp"
                    android:layout_height="100sp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/sports"
                        android:scaleType="centerCrop"/>



                </LinearLayout>



            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sports"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:textSize="18sp"/>


        </LinearLayout>

    </LinearLayout>



</LinearLayout>